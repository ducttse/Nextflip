
@{
    Layout = "_CustomLayout";
}
@section Styles {
    <link href="@Url.Content("~/css/View-Ticket-Details.css")" rel="stylesheet" type="text/css" />
}
<section class="w-75 h-100 mx-auto">
    <div class="col-2">
        <a
          id="back_btn"
          href="/SupporterDashboard/Index"
          class="btn btn-warning text-decoration-none link-light mx-auto w-50"
          ><i class="fas fa-arrow-left"></i> Back</a
        >
    </div>
    <div class="col-12 mt-3 shadow-lg p-3 mb-5 bg-body rounded" id="contentWrapper">
    </div>
</section>
<script src="~/js/View-Ticket-Details.js"></script>
<script>
    Run('@ViewBag.ticketId');
</script>