<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!-- bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css"
          rel="stylesheet"
          integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x"
          crossorigin="anonymous" />
    <!-- font awesome -->
    <script src="https://kit.fontawesome.com/c82ac77d9b.js"
            crossorigin="anonymous"></script>
    <meta name="google-signin-client_id" content="753240362122-b5jcp3ipgeuo2alsicq580puevmjgp91.apps.googleusercontent.com">
    <script src="https://apis.google.com/js/platform.js?onload=init" async defer></script>
    <title>@ViewBag.Title</title>
    @RenderSection("styles", false)
</head>
<body>
    <partial name="~/Views/SharedComponents/UserDashboardHeader.cshtml" />
    @RenderBody()
    @RenderSection("PreScript", false)
    @RenderSection("script", false)
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4"
            crossorigin="anonymous"></script>
    <script>
    function signOut() {
        var auth2 = gapi.auth2.getAuthInstance();
        console.log(auth2);
        auth2.signOut().then(windonw.location.replace("/Login/Index"))
    }
    function init() {
        gapi.load('auth2', function() {
        /* Ready. Make a call to gapi.auth2.init or some other API */
        });
    }
    init();
</script>
</body>
</html>

